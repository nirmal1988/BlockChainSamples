tr#partsPanelTR(style="display:none;")
	td
		h5 Service:
	td
		legend(style="display:none;")
			select#selectedParts(style="float:left;text-align:left;")
			div#upParts(style="float:left;text-align:left;margin-left:6px; padding:5px;border:solid 1px #ccc;float:left;")			
		legend(style="display:block;")
			table
				tr
					td(style="width: 95px;") Service Done:
					td(style="text-align:left;")
						input#upServiceDone(type="checkbox" name="upServiceDone" style="margin-left: 7px;") 
				tr
					td(style="width: 95px;") Service Note:
					td(style="text-align:left;")
						textarea(id="upServiceDesc" style="margin-left: 7px;")
				tr
					td(style="width: 95px;" valign="top") Parts:
					td(style="text-align:left;")
						input(type="text" name="upPartId" placeholder="Part Id")
						button(type="button" style="margin:5px" )#btnAddPart
							span Add
						br
						div#upPartsAddedBySC(style="width:350px;float:left;text-align:left;margin-left:-2px;padding:5px;border:solid 0px #ccc;float:left;")			

